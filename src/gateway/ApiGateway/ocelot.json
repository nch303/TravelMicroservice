{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/userservice/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "userservice",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/userservice/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "SwaggerKey": "users"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "users",
      "Config": [
        {
          "Name": "User Service",
          "Version": "v1",
          "Url": "http://userservice/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://143.198.203.34:5120"
  }
}